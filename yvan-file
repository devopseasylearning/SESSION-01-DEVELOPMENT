cat /var/log/secure | grep "Failed"  | awk '{print $9}' | sort -u
cat /var/log/secure | grep "Failed"  | awk '{print $9}' | sort -u |wc -l
cat /var/log/secure | grep "Failed"  | awk '{print $9}' | sort -u |wc -l > users.t
